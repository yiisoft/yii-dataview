        "irc": "irc://irc.freenode.net/yii"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4",
        "yiisoft/arrays": "^3.0@dev",
        "yiisoft/strings": "^3.0@dev",
        "yiisoft/view": "^3.0@dev",
        "yiisoft/factory": "^3.0@dev",
        "yiisoft/data": "^3.0@dev",
        "ext-json": "*"
    },
    "require-dev": {
        "yiisoft/di": "^3.0@dev",
        "yiisoft/event-dispatcher": "^3.0@dev",
        "yiisoft/log": "^3.0@dev",
        "yiisoft/cache": "^3.0@dev",
        "phpunit/phpunit": "^8.4",
        "friendsofphp/php-cs-fixer": "^2.2",
        "hiqdev/composer-config-plugin": "^1.0@dev"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        },
        "config-plugin": {
            "tests": "config/tests.php"
        }
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Tests\\": "vendor/yiisoft/view/tests",
            "Yiisoft\\Yii\\DataView\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Yii\\DataView\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
